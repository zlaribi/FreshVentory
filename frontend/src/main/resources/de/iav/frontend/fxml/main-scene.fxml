<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="tableView" minHeight="-Infinity" minWidth="-Infinity" prefHeight="780.0" prefWidth="360.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.iav.frontend.controller.MainPageController">
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="10.0"/>
   </padding>
   <children>
      <Button mnemonicParsing="false" onAction="#switchToAddFoodScene" text="New Food"/>
      <Button fx:id="deleteFoodById" mnemonicParsing="false" onAction="#deleteFoodById" text="Delete Food"/>
      <Button mnemonicParsing="false" onAction="#switchToCheckDateScene" text="Check Date"/>
      <TableView fx:id="table" prefHeight="522.0" prefWidth="280.0">
         <columns>
            <TableColumn fx:id="nameColumn" prefWidth="66.0" text="Name"/>
            <TableColumn fx:id="categoryColumn" prefWidth="89.0" text="Category"/>
            <TableColumn fx:id="quantityColumn" prefWidth="54.0" text="Quantity"/>
            <TableColumn fx:id="expirationDateColumn" prefWidth="70.0" text="Date"/>
         </columns>
      </TableView>
      <Button fx:id="updateFoodButton" mnemonicParsing="false" onAction="#switchToUpdateFoodScene" text="Edit Food"/>
   </children>
</VBox>
